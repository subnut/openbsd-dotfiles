! Xft.dpi: 96
! Xcursor.theme: capitaine-cursors-light
Xcursor.size: 64


!!!!! URxvT !!!!!!!!!!!!!!!!!!!!!!!!!!
!-------------------------------------

URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.iso14755: true
URxvt.iso14755_52: false

URxvt.intensityStyles: false
URxvt.skipBuiltinGlyphs: true

URxvt.font:  xft:Recursive Mn Csl St:pixelsize=32.5, \
             xft:MesloLGS NF:style=Regular


! The following colors have been taken from
! man 7 urxvt, line number 356
!
! A quicker way to reach it is -
!       man 7 urxvt
! And then, in less, search for Xdefaults -
!       /Xdefaultsâ†©
! And you should have reached it.

URxvt.color0:   #000000
URxvt.color1:   #A80000
URxvt.color2:   #00A800
URxvt.color3:   #A8A800
URxvt.color4:   #0000A8
URxvt.color5:   #A800A8
URxvt.color6:   #00A8A8
URxvt.color7:   #CFCFCF
! Yes, I modified color7 a bit :D
! But that's because vim uses it as the background for selections
! And trust me, #A8A8A8 doesn't look good as a background
! ... you can trust me, right? :D

URxvt.color8:   #000054
URxvt.color9:   #FF0054
URxvt.color10:  #00FF54
URxvt.color11:  #FFFF54
URxvt.color12:  #0000FF
URxvt.color13:  #FF00FF
URxvt.color14:  #00FFFF
URxvt.color15:  #FFFFFF

